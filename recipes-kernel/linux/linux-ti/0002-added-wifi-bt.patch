From e61f34b10d0663f72ee275e94a8394ced0791f94 Mon Sep 17 00:00:00 2001
From: Kadmobile <kaderkhafif@gmail.com>
Date: Mon, 22 May 2023 13:57:16 -0700
Subject: [PATCH 2/5] added wifi bt

---
 arch/arm64/boot/dts/ti/k3-am62-evergreen.dtsi | 93 +++++++++++++++++++
 1 file changed, 93 insertions(+)

diff --git a/arch/arm64/boot/dts/ti/k3-am62-evergreen.dtsi b/arch/arm64/boot/dts/ti/k3-am62-evergreen.dtsi
index ea4151a11269..aebe60ab4920 100644
--- a/arch/arm64/boot/dts/ti/k3-am62-evergreen.dtsi
+++ b/arch/arm64/boot/dts/ti/k3-am62-evergreen.dtsi
@@ -9,7 +9,22 @@
 #include <dt-bindings/leds/common.h>
 #include <dt-bindings/net/ti-dp83867.h>
 
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/pinctrl/k3.h>
+
 / {
+		reg_wifi_en: fixed-regulator-wifi-en {
+		compatible = "regulator-fixed";
+		regulator-name = "wifi_en";
+		regulator-min-microvolt = <1800000>;
+		regulator-max-microvolt = <1800000>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&wifi_pins_default>;
+		regulator-always-on;
+		enable-active-high;
+		gpio = <&main_gpio0 72 GPIO_ACTIVE_HIGH>;
+	};
+
 	aliases {
 		serial2 = &main_uart0;
 		mmc0 = &sdhci0;
@@ -130,6 +145,47 @@
 };
 
 &main_pmx0 {
+	main_uart3_bt_pins_default: main-uart3-bt-pins-default {
+		pinctrl-single,pins = <
+			AM62X_IOPAD(0x00C0, PIN_INPUT, 4) /* (W25) VOUT0_DATA2.UART3_RXD */
+			AM62X_IOPAD(0x00C4, PIN_OUTPUT, 4) /* (W24) VOUT0_DATA3.UART3_TXD */
+			AM62X_IOPAD(0x00FC, PIN_INPUT, 4) /* (Y20) VOUT0_DE.UART3_CTSn */
+			AM62X_IOPAD(0x00F8, PIN_OUTPUT, 4) /* (AB24) VOUT0.HSYNC.UART3_RTSn */
+		>;
+	};
+
+	bt_pins_default: bt-pins-default {
+		pinctrl-single,pins = <
+			/* BT_EN */
+			AM62X_IOPAD(0x124, PIN_OUTPUT, 7) /* (A23) MMC2_SDCD.GPIO0_71 */
+			/* BT_HOST_WAKE */
+			AM62X_IOPAD(0x09c, PIN_OUTPUT, 7) /* (V25) GPMC0_WAIT1.GPIO0_38 */
+			/* BT_DEV_WAKE */
+			AM62X_IOPAD(0x1f0, PIN_OUTPUT, 7) /* (A18) EXT_REFCLK1.GPIO1_30 */
+		>;
+	};
+
+	main_mmc2_wifi_pins_default: main-mmc2-wifi-pins-default {
+		pinctrl-single,pins = <
+			AM62X_IOPAD(0x108, PIN_INPUT, 0) /* (D24) MMC2_DAT3 */
+			AM62X_IOPAD(0x10c, PIN_INPUT, 0) /* (E23) MMC2_DAT2 */
+			AM62X_IOPAD(0x110, PIN_INPUT, 0) /* (C25) MMC2_DAT1 */
+			AM62X_IOPAD(0x114, PIN_INPUT, 0) /* (B24) MMC2_DAT0 */
+			AM62X_IOPAD(0x118, PIN_INPUT, 0) /* (D25) MMC2_CLK */
+			AM62X_IOPAD(0x120, PIN_INPUT, 0) /* (C24) MMC2_CMD */
+			AM62X_IOPAD(0x11c, PIN_INPUT, 0) /* (#N/A) MMC2_CLKB */
+		>;
+	};
+
+	wifi_pins_default: wifi-pins-default {
+		pinctrl-single,pins = <
+			/* WIFI_EN */
+			AM62X_IOPAD(0x128, PIN_OUTPUT, 7) /* (B23) MMC2_SDWP.GPIO0_72 */
+			/* WIFI_HOST_WAK */
+			AM62X_IOPAD(0x044, PIN_INPUT, 7) /* (N24) GPMC0_AD2.GPIO0_17 */
+		>;
+	};
+
 	audio_ext_refclk1_pins_default: audio-ext-refclk1-pins-default {
 		pinctrl-single,pins = <
 			AM62X_IOPAD(0x0a0, PIN_OUTPUT, 1) /* (K25) GPMC0_WPn.AUDIO_EXT_REFCLK1 */
@@ -265,6 +321,43 @@
 	};
 };
 
+&sdhci2 {
+	#address-cells = <1>;
+	#size-cells = <0>;
+
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&main_mmc2_wifi_pins_default>;
+	vmmc-supply = <&reg_wifi_en>;
+	bus-width = <4>;
+	non-removable;
+
+	ti,fails-without-test-cd;
+	cap-power-off-card;
+	keep-power-in-suspend;
+	ti,driver-strength-ohm = <50>;
+	assigned-clocks = <&k3_clks 157 158>;
+	assigned-clock-parents = <&k3_clks 157 160>;
+
+	wlcore: wlcore@2 {
+		compatible = "ti,wl1837";
+		reg = <2>;
+		interrupt-parent = <&main_gpio0>;
+		interrupts = <17 0>;
+	};
+};
+
+&main_uart3 {
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&main_uart3_bt_pins_default &bt_pins_default>;
+
+	bluetooth {
+		compatible = "ti,wl1837-st";
+		enable-gpios = <&main_gpio0 71 GPIO_ACTIVE_HIGH>;
+	};
+};
+
 &cpsw3g {
 	pinctrl-names = "default";
 	pinctrl-0 = <&main_mdio1_pins_default
-- 
2.17.1

